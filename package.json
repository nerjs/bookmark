{
  "name": "bookmarks",
  "version": "0.0.1",
  "description": "Bookmarks app for React.js, redux, graphql, Apollo, Node.js, mongodb, Mongoose",
  "main": "index.js",
  "scripts": {
    "test": "nodemon --exec babel-node ./server/index.js"
  },
  "keywords": [
    "React.js",
    "redux",
    "graphql",
    "Apollo",
    "Node.js",
    "mongodb",
    "Mongoose"
  ],
  "author": "nerjs",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "src/*",
      "node_modules/*"
    ],
    "delay": "1500",
    "env": {
      "NODE_ENV": "development",
      "NODE_PATH": "./server/lib"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "body-parser": "^1.18.3",
    "colors": "^1.3.0",
    "cookie-parser": "^1.4.3",
    "crypto": "^1.0.1",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-middleware": "^1.5.0",
    "graphql-tools": "^3.0.5",
    "jsonwebtoken": "^8.3.0",
    "merge-graphql-schemas": "^1.5.2",
    "mongoose": "^5.2.1",
    "morgan": "^1.9.0"
  }
}
